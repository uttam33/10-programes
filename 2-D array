#include<stdio.h>
#include<conio.h>
int main()
{
	int a[2][2],i,j;
	printf("\nenter the value of array\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
		scanf("%d\n",&a[i][j]);
		}
	}
	printf("\nentered value of array is : \n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	}
	getch();
	return 0;
}
