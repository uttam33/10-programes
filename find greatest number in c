if(a>b && a>c)
{
printf("%d is greatest all among three\n");	
}
else if(b>a && b>c)
{
printf("%d is greatest all among three\n");	
}
else if(c>a && c>a)
{
printf("%d is greatest all among three\n");	
}
getch();
