#include<stdio.h>
#include<conio.h>
int main()
{
	int a[50],b[50],c[50],i,n;
	printf("\nenter the value of array\n");
	scanf("%d",&n);
	printf("enter the element of a\n");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
    printf("enter the element of b\n");
	for(i=0;i<n;i++)
	scanf("%d",&b[i]);
	for(i=0;i<n;i++)
	c[i]=a[i]+b[i];
	for(i=0;i<n;i++)
	printf(" sum is = %d\n",c[i]);
	getch();
	return 0;
}
